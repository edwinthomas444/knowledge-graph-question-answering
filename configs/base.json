{
    "dataset":{
        "ent_file": "../KB_v5/entity_csv.xlsx",
        "prop_file": "../KB_v5/predicate_csv.xlsx",
        "trip_file": "../KB_v5/triplet_csv.xlsx",
        "train_ds_file": "../Dataset_v4/train.txt",
